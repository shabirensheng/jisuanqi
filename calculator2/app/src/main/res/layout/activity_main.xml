<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/activity_main"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context="com.example.lhb.calculator.MainActivity"
android:weightSum="1">


<TextView
    android:id="@+id/res"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:height="200dp"
    android:layout_margin="20px"
    android:textSize="60px"

    />

<Button
    android:id="@+id/clean"
    android:layout_width="350px"
    android:layout_height="wrap_content"
    android:layout_below="@+id/res"
    android:layout_marginRight="10px"
    android:layout_marginLeft="20px"
    android:text="Clean"
    android:textSize="50px"
    />

<Button
    android:id="@+id/back"
    android:layout_width="350px"
    android:layout_height="wrap_content"
    android:layout_toRightOf="@+id/clean"
    android:layout_alignTop="@+id/clean"
    android:layout_marginRight="20px"
    android:text="Back"
    android:textSize="50px"
    />

<Button
    android:id="@+id/button9"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@+id/tentotwo"
    android:layout_marginRight="10px"
    android:layout_marginLeft="10px"
    android:layout_marginTop="10px"
    android:text="9"
    android:textSize="50px"
    />

<Button
    android:id="@+id/button8"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_toRightOf="@+id/button9"
    android:layout_alignTop="@+id/button9"
    android:layout_marginRight="10px"
    android:text="8"
    android:textSize="50px"
    />
<Button
    android:id="@+id/button7"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_toRightOf="@+id/button8"
    android:layout_alignTop="@+id/button9"
    android:layout_marginRight="10px"
    android:text="7"
    android:textSize="50px"
    />

<Button
    android:id="@+id/button6"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@+id/button9"
    android:text="6"
    android:textSize="50px"

    android:layout_marginRight="10px"
    android:layout_marginLeft="10px"
    android:layout_marginTop="10px"/>
<Button
    android:id="@+id/button5"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_toRightOf="@+id/button6"
    android:layout_alignTop="@+id/button6"
    android:layout_marginRight="10px"
    android:text="5"
    android:textSize="50px"
    />
<Button
    android:id="@+id/button4"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_toRightOf="@+id/button5"
    android:layout_alignTop="@+id/button6"
    android:layout_marginRight="10px"
    android:text="4"
    android:textSize="50px"
    />

<Button
    android:id="@+id/button3"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@+id/button6"
    android:text="3"
    android:textSize="50px"

    android:layout_marginRight="10px"
    android:layout_marginLeft="10px"
    android:layout_marginTop="10px"/>
<Button
    android:id="@+id/button2"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_toRightOf="@+id/button3"
    android:layout_alignTop="@+id/button3"
    android:layout_marginRight="10px"
    android:text="2"
    android:textSize="50px"
    />
<Button
    android:id="@+id/button1"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_toRightOf="@+id/button2"
    android:layout_alignTop="@+id/button3"
    android:layout_marginRight="10px"
    android:text="1"
    android:textSize="50px"

    />

<Button
    android:id="@+id/button0"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@+id/button3"

    android:layout_marginRight="10px"
    android:layout_marginLeft="10px"
    android:layout_marginTop="10px"
    android:text="0"
    android:textSize="50px"
    />

<Button
    android:id="@+id/add"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_toRightOf="@+id/button7"
    android:layout_alignTop="@+id/button9"
    android:layout_marginRight="10px"
    android:text="+"
    android:textSize="50px"
    />

<Button
    android:id="@+id/sub"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_toRightOf="@+id/button4"
    android:layout_alignTop="@+id/button6"
    android:layout_marginRight="10px"
    android:text="-"
    android:textSize="50px"
    />
<Button
    android:id="@+id/mul"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_toRightOf="@+id/button1"
    android:layout_alignTop="@+id/button3"
    android:layout_marginRight="10px"
    android:text="*"
    android:textSize="50px"
    />
<Button
    android:id="@+id/div"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_toRightOf="@+id/equal"
    android:layout_alignTop="@+id/button0"
    android:layout_marginRight="10px"
    android:text="/"
    android:textSize="50px"
    />

<Button
    android:id="@+id/spot"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_toRightOf="@+id/button0"
    android:layout_alignTop="@+id/button0"
    android:layout_marginRight="10px"
    android:text="."
    android:textSize="50px"
    />
<Button
    android:id="@+id/equal"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_toRightOf="@+id/spot"
    android:layout_alignTop="@+id/button0"
    android:layout_marginRight="10px"
    android:text="="
    android:textSize="50px"

    />
    <Button
        android:id="@+id/tentotwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/clean"

        android:layout_marginRight="10px"
        android:layout_marginLeft="10px"
        android:layout_marginTop="10px"
        android:text="10to2"
        android:textSize="30px"
        />
    <Button
        android:id="@+id/twototen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/tentotwo"
        android:layout_alignTop="@+id/tentotwo"
        android:layout_marginRight="10px"
        android:text="2to10"
        android:textSize="30px"
        />
    <Button
        android:id="@+id/tentoeig"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/twototen"
        android:layout_alignTop="@+id/tentotwo"
        android:layout_marginRight="10px"
        android:text="10to8"
        android:textSize="30px"
        />
    <Button
        android:id="@+id/eigtoten"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/tentoeig"
        android:layout_alignTop="@+id/tentotwo"
        android:layout_marginRight="10px"
        android:text="8to10"
        android:textSize="30px"
        />

</RelativeLayout>